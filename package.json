{
  "name": "antiflux",
  "version": "0.1.24",
  "keywords": [
    "store",
    "state",
    "observable",
    "cache",
    "config",
    "node",
    "flux"
  ],
  "author": "Daniel Sieradski <daniel@self.agency>",
  "repository": {
    "type": "git",
    "url": "https://github.com/selfagency/antiflux"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "main": "dist/index.js",
  "types:": "dist/index.d.ts",
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf dist; NODE_ENV=production webpack; cp src/index.d.ts dist/index.d.ts",
    "test": "jest --coverage"
  },
  "jest": {
    "testTimeout": 10000,
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-loader": "^8.2.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.3",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "source-map-loader": "^3.0.1",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {}
}
